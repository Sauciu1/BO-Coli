#!/bin/bash
#PBS -l select=1:ncpus=96:mem=256gb -l walltime=05:00:00

cd povilas/BO-Coli

source "$HOME/miniforge3/etc/profile.d/conda.sh"

conda create -y "bo_coli" python=3.13.0 ipython
conda activate bo_coli
conda install pip
pip install poetry

poetry lock

poetry install --no-root
poetry run python h6_simulations.py

